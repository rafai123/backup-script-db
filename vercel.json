{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/backup",
        "dest": "index.js"
      }
    ],
    "crons": [
      {
        "path": "/api/backup",
        "schedule": "0 * * * *"
      }
    ]
  }
  